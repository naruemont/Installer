#!/bin/sh


credentials: 
	test -f ~/.ssh/id_rsa || (				\
	read -p "Please Enter Your Email:" email;	\
	ssh-keygen -t rsa -b 4096 -C $$email);)
	eval `ssh-agent` && ssh-add ~/.ssh/id_rsa
	@echo "Please Add SSH key to your github account:" && cat ~/.ssh/id_rsa.pub


	
install:
	@echo "Initialization Download Deversifi Middleware API Project" && 	\
	curl -L https://raw.githubusercontent.com/naruemont/ExchangeLogExport/master/GET_TRADE_HISTORY_BITFINEX/GET_TRADE_HISTORY_BITFINEX.tar.gz | tar xz && chmod +x *
	@echo "Finished Installation"


	

